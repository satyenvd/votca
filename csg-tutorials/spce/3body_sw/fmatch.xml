<cg>
  <!-- nbsearch grid is default setting -->
  <nbsearch>grid</nbsearch>
 
  <!--fmatch section -->
  <fmatch>
    <!--Number of frames for block averaging -->
    <frames_per_block>20</frames_per_block>
    <!--Use constrained least squares fitting -->
    <constrainedLS>true</constrainedLS>
  </fmatch>

  <!-- non-bonded interaction entry -->
  <non-bonded>
   <!-- name of the interaction -->
    <name>CG-CG</name>
    <type1>CG</type1>
    <type2>CG</type2>
    <fmatch>
      <min>0.24</min>
      <max>1.2</max>
      <step>0.01</step>
      <out_step>0.001</out_step>
    </fmatch>
  </non-bonded>
  
  <!-- non-bonded interaction entry -->
  <non-bonded>
   <!-- name of the interaction -->
    <name>CG-CG-CG</name>
    <threebody>1</threebody>
    <type1>CG</type1>
    <type2>CG</type2>
    <type3>CG</type3>
    <fmatch>
      <a>0.37</a>
      <sigma>1.0</sigma>
      <gamma>0.08</gamma>
      <min>0.7194247283</min>
      <max>3.1415927</max>
      <step>0.1</step>
      <out_step>0.0031415927</out_step>
    </fmatch>
  </non-bonded>
</cg>
